<h1>Users</h1>
<a href="/users/create">Create</a>
<form action="/users/search" method="GET">
    <input type="text" name="q" id="keyword">
    <button type="submit">Search</button>
</form>
<ul>
    {{#each users}}
    <li>
        <span>{{this.name}} &nbsp;</span>
        <a href="/users/" + {{this.id}}>Detail</a>
    </li>
    {{/each}}
</ul>